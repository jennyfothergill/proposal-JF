isbn = {0021-9606},
issn = {00219606},
journal = {Journal of Chemical Physics},
number = {4},
pages = {1702--1709},
title = {{An Optimized United Atom Model for Simulations of polymethylene Melts}},
url = {http://scitation.aip.org/content/aip/journal/jcp/103/4/10.1063/1.469740{\%}5Cnhttp://dx.doi.org/10.1063/1.469740{\%}5Cnhttp://link.aip.org/link/JCPSA6/v103/i4/p1702/s1{\&}Agg=doi},
