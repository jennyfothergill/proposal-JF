issn = {15214095},
journal = {Advanced Materials},
keywords = {flexibility,impulse calculation,modules,organic solar cells,slot-die printing},
number = {41},
pages = {1--10},
title = {{A General Approach for Lab-to-Manufacturing Translation on Flexible Organic Solar Cells}},
volume = {31},
year = {2019}
}
