@article{Zhao2013b,
abstract = {The influence of polymer entanglement on the self-assembly, molecular packing structure, and microstructure of low-M-w (lightly entangled) and high-M-w (highly entangled) poly (3-hexylthiophene) (P3HT), and the carrier transport in thin-film transistors, are investigated. The polymer chains are gradually disentangled in a marginal solvent via ultrasonication of the polymer solution, and demonstrate improved diffusivity of precursor species (coils, aggregates, and microcrystallites), enhanced nucleation and crystallization of P3HT in solution, and self-assembly of well-ordered and highly textured fibrils at the solid-liquid interface. In low-M-w P3HT, reducing chain entanglement enhances interchain and intrachain ordering, but reduces the interconnectivity of ordered domains (tie molecules) due to the presence of short chains, thus deteriorating carrier transport even in the face of improving crystallinity. Reducing chain entanglement in high-M-w P3HT solutions increases carrier mobility up to approximate to 20-fold, by enhancing interchain and intrachain ordering while maintaining a sufficiently large number of tie molecules between ordered domains. These results indicate that charge carrier mobility is strongly governed by the balancing of intrachain and interchain ordering, on the one hand, and interconnectivity of ordered domains, on the other hand. In high-M-w P3HT, intrachain and interchain ordering appear to be the key bottlenecks to charge transport, whereas in low-M-w P3HT, the limited interconnectivity of the ordered domains acts as the primary bottleneck to charge transport.},
author = {Zhao, Kui and Khan, Hadayat Ullah and Li, Ruipeng and Su, Yisong and Amassian, Aram},
doi = {10.1002/adfm.201301007},
file = {:Users/jenny/Documents/Mendeley Desktop/Zhao et al.{\_}2013{\_}Advanced Functional Materials.pdf:pdf},
isbn = {1616-301X},
issn = {1616301X},
journal = {Advanced Functional Materials},
keywords = {P3HT,entanglement,molecular self-assembly,organic transistors,thin films},
number = {48},
pages = {6024--6035},
title = {{Entanglement of conjugated polymer chains influences molecular self-assembly and carrier transport}},
volume = {23},
year = {2013}
}
