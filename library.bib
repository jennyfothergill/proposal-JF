journal = {Journal of Chemical Information and Modeling},
month = {apr},
number = {4},
pages = {944--955},
publisher = {American Chemical Society},
title = {{Hydrogen bonding and $\pi$-stacking: How reliable are force fields? A critical evaluation of force field descriptions of nonbonded interactions}},
url = {http://pubs.acs.org/doi/abs/10.1021/ci900009f},
volume = {49},
year = {2009}
