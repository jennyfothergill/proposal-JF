keywords = {crystal structure,density functional calculations,molecular modelling,non-fullerene,solar cells},
number = {20},
pages = {2608--2626},
title = {{Building Blocks for High-Efficiency Organic Photovoltaics: Interplay of Molecular, Crystal, and Electronic Properties in Post-Fullerene ITIC Ensembles}},
volume = {20},
year = {2019}
}
